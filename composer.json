{
  "name": "abergasov/github_client",
  "description": "GitHub GraphQL API client",
  "keywords": ["github", "api", "graphql", "client"],
  "homepage": "https://github.com/abergasov/github_client",
  "license": "MIT",
  "authors": [
    {
      "name": "Alexander Bergasov",
      "email": "abergasov@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.1.0",
    "ext-curl": "*",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.5 || ^6.0"
  },
  "autoload": {
    "psr-4": {
      "Github\\": "src/Github/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "GitHub\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit"
  }
}
